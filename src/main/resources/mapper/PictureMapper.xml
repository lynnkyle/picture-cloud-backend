<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.picturecloudbackend.mapper.PictureMapper">

    <resultMap id="BaseResultMap" type="org.example.picturecloudbackend.model.entity.Picture">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="picName" column="pic_name" jdbcType="VARCHAR"/>
            <result property="picUrl" column="pic_url" jdbcType="VARCHAR"/>
            <result property="picIntro" column="pic_intro" jdbcType="VARCHAR"/>
